<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Qwer\LottoDocumentsBundle\Entity\BetLine" table="lotto_bet_line">
    <field name="balls" type="string" column="balls" length="100" nullable="true"/>
    <field name="withBonus" type="boolean" column="with_bonus" nullable="true"/>
    <many-to-one field="lottoDraw" target-entity="Qwer\LottoBundle\Entity\Draw">
      <join-columns>
        <join-column name="lotto_draw_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
