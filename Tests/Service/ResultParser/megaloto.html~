 <script>
    $(document).ready(function(){
        if (sessionStorage.getItem('is18') != 1) {
        $('._18_1').parent().show();
        $('._18_1').fadeIn();

        $('._18_yes a').click(function(e){
            e.preventDefault();
            $('._18_1').parent().fadeOut();
            sessionStorage.setItem('is18', '1');
        });
    }

    });
    //testing
</script>

