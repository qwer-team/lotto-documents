<script>
$(document).ready(function(){
	var  closer;
	var current_l;
	var current_b;
        var openCheck;
        var dempfer=0;
        var dempfer_time=200;
	 function close_it(video){
	 	 if (video)
                {
                var left_p=current_l.offset().left-560;
                }
                else var left_p=current_l.offset().left-70;
		var top_p=0-current_b.height()-50;
		current_l.css('font-weight', 'normal');
		current_b.css('left', left_p);
		current_b.animate({top: top_p},300);
	 }
	 
	 function drive(video)
	 {
                if (video)
                {
                var left_p=current_l.offset().left-560;
                }
                else var left_p=current_l.offset().left-70;
		var top_p='91px';
		current_l.css('font-weight', 'normal');
		current_b.css('left', left_p);
		current_b.animate({top: top_p},300);
	 }
	$('#tm_1, #tm_2, #tm_3').mouseout( function(){ clearTimeout(openCheck); });

	$('#tm_1').hover(function(event){
		clearTimeout(closer);
		current_l=$('#tm_2');
		current_b=$('.dd_m_2');
		close_it(1);
		current_l=$('#tm_3');
		current_b=$('.dd_m_3');
		close_it();
		current_l=$('#tm_4');
		current_b=$('.dd_m_4');
		close_it();
		current_l=$('#tm_1');
		current_b=$('.dd_m_1');
		openCheck=setTimeout(function(){drive()}, dempfer_time);
	});
	
	$('.dd_m_1').mouseover(function(){
		clearTimeout(closer);
	});
	$('.dd_m_1').mouseout(function(){
		closer = setTimeout(function(){close_it()}, 700);
	});
	
	$('#tm_1').mouseout(function(event){
		closer = setTimeout(function(){close_it()}, 700);
		
	});
	
	// ////////////////////////////////
	
	$('#tm_2').hover(function(event){
		clearTimeout(closer);
		current_l=$('#tm_1');
		current_b=$('.dd_m_1');
		close_it();
		current_l=$('#tm_4');
		current_b=$('.dd_m_4');
		close_it();
		current_l=$('#tm_3');
		current_b=$('.dd_m_3');
		close_it();
		current_l=$('#tm_2');
		current_b=$('.dd_m_2');
		openCheck=setTimeout(function(){drive(1)}, dempfer_time);
	});
	$('.dd_m_2').mouseover(function(){
		clearTimeout(closer);
	});
	$('.dd_m_2').mouseout(function(){
		closer = setTimeout(function(){close_it(1)}, 700);
	});
	
	$('#tm_2').mouseout(function(event){
		closer = setTimeout(function(){close_it(1)}, 700);
		
	});
	
	// /////////////////////////////
	
	$('#tm_3').hover(function(event){
		clearTimeout(closer);
		current_l=$('#tm_1');
		current_b=$('.dd_m_1');
		close_it();
		current_l=$('#tm_2');
		current_b=$('.dd_m_2');
		close_it(1);
		current_l=$('#tm_4');
		current_b=$('.dd_m_4');
		close_it();
		current_l=$('#tm_3');
		current_b=$('.dd_m_3');
		openCheck=setTimeout(function(){drive()}, dempfer_time);
	});
	$('.dd_m_3').mouseover(function(){
		clearTimeout(closer);
	});
	$('.dd_m_3').mouseout(function(){
		closer = setTimeout(function(){close_it()}, 700);
	});
	
	$('#tm_3').mouseout(function(event){
		closer = setTimeout(function(){close_it()}, 700);
		
	});
	
	// /////////////////////////////

$('#tm_4').hover(function(event){
		clearTimeout(closer);
		current_l=$('#tm_1');
		current_b=$('.dd_m_1');
		close_it();
		current_l=$('#tm_2');
		current_b=$('.dd_m_2');
		close_it(1);
		current_l=$('#tm_3');
		current_b=$('.dd_m_3');
		close_it();
		current_l=$('#tm_4');
		current_b=$('.dd_m_4');
		openCheck=setTimeout(function(){drive()}, dempfer_time);
	});
	$('.dd_m_4').mouseover(function(){
		clearTimeout(closer);
	});
	$('.dd_m_4').mouseout(function(){
		closer = setTimeout(function(){close_it()}, 700);
	});
	
	$('#tm_4').mouseout(function(event){
		closer = setTimeout(function(){close_it()}, 700);
		
	});
	
	// /////////////////////////////
	

});

</script>

