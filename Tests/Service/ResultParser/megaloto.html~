		<script type="text/javascript">
			var selectedNumbers = new Array();
			var numbersLimit = 10;

			$('table#af_table_table td.pseudo_link').click(function() {
				if ($(this).hasClass('number_active') && (selectedNumbers.length >= 0)) {
					$(this).removeClass('number_active');
					var idx = selectedNumbers.indexOf($(this).text());
					if (idx != -1) selectedNumbers.splice(idx, 1);
				}
				else if (selectedNumbers.length < numbersLimit) {
					$(this).addClass('number_active');
					selectedNumbers.push($(this).text());
				}
			});
		</script>
