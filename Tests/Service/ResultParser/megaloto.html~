<div id="but_rospis_inact" class="but_selected"><span class="anal_active">Розпис</span></div>
<div id="but_rospis" class="but_not_selected hidden"><a href="#" class="anal_yellow_link">Розпис</a></div>
<div id="but_numbers_inact" class="but_selected hidden"><span class="anal_active">Улюбленi числа</span></div>
<div id="but_numbers" class="but_not_selected"><a href="#" class="anal_yellow_link">Улюбленi числа</a></div>

<script type="text/javascript">
/* $(document).ready(function(){
height_correction_1();
height_correction_2();
}); */
	function height_correction_1() {
		var new_height_block = 0;
		var new_height_results = 0;
		var new_height = $("#var1_item_6 div.analitics_result").height();
		var new_width = $("#var1_item_6 div.analitics_result").width();
		//alert(new_height);
		if (new_height < 130) new_height = 180;

		if ($("#var1_item_6 .analitics_result").text().length > 0) {
			if (new_height >= 580) {
				new_height_block = '580';
				new_height_results = new_height_block - 137;
			}
			else {
				new_height_block = new_height + 190;
				new_height_results = new_height + 50;
			}

			$("#analitics").animate({height: new_height_block}, 300);
			$("#var1_item_6").css("height", new_height_results);
			$("#var1_item_6").css("width", new_width);
		}
	}

	function height_correction_2() {
		var new_height_block = 0;
		var new_height_results = 0;
		var new_height = $("#var2_item_6 div.analitics_result").height();
		var new_width = $("#var2_item_6 div.analitics_result").width();
		//alert(new_height);
		if (new_height < 120) new_height = 150;
		if ($("#var2_item_6 .analitics_result").text().length > 0) {
			if (new_height >= 580) {
				new_height_block = '580';
				new_height_results = new_height_block - 137;
			}
			else if (new_height > 212) {
				new_height_block = new_height + 190;
				new_height_results = new_height + 50;
			}
			else {
				new_height_block = 383;
				new_height_results = new_height + 50;
			}

			$("#analitics").animate({height: new_height_block}, 300);
			$("#var2_item_6").css("height", new_height_results - 20);
			$("#var2_item_6").css("width", new_width+20);
		}
	}

	function resetNumbers() {
		$('table#af_table_table td.pseudo_link').removeClass('number_active');
		$('#af_table_mball select :first').attr("selected", "selected");
		selectedNumbers = new Array();
	}
	function resetResults() {
		$("div#af_variant_1 div.analitics_result").html('');
		$("div#af_variant_2 div.analitics_result").html('');
				height_correction_1();
		height_correction_2();
	}

	$("#var1_item_10").click(function (ev) {
		resetNumbers();
		resetResults();


	});

	$("#but_rospis a").click(function (ev) {
		ev.preventDefault();

		$("div#but_rospis_inact").show();
		$("div#but_numbers").show();
		$("div#but_rospis").hide();
		$("div#but_numbers_inact").hide();
		$("div#af_variant_1").show();
		$("div#af_variant_2").hide();
		$("div#af_table_title span#af_numbers").hide();
		$("div#af_table_title span#af_rospis").show();
		
		resetNumbers();
		resetResults();

		numbersLimit = 10;
		height_correction_2();
	});

	$("#but_numbers a").click(function (ev) {
		ev.preventDefault();

		$("div#but_rospis").show();
		$("div#but_numbers_inact").show();
		$("div#but_rospis_inact").hide();
		$("div#but_numbers").hide();
		$("div#af_variant_2").show();
		$("div#af_variant_1").hide();
		$("div#af_table_title span#af_numbers").show();
		$("div#af_table_title span#af_rospis").hide();
		

		resetNumbers();
		resetResults();

		numbersLimit = 6;
		height_correction_1();
	});

	$("div#af_variant_1 form").submit(function(ev) {
		ev.preventDefault();
		if ((selectedNumbers.length < 7) || (selectedNumbers.length > 10)) {
			var textError = '0 чисел';
			if (selectedNumbers.length == 1) {
				textError = selectedNumbers.length + ' число';
			} else if (selectedNumbers.length > 1 && selectedNumbers.length <= 4) {
				textError = selectedNumbers.length + ' числа';
			} else if (selectedNumbers.length > 4) {
				textError = selectedNumbers.length + ' чисел';
			}
			$(this).find("div.analitics_result").html("");
			return $(this).find('.errorResult').html('Ви обрали ' + textError + ', а необхідно обрати від 7 до 10 чисел');
			//return alert('Ви обрали меньше сіми номерів');
		}
		if (!$('select[name="megaball"]').val()) {
			$(this).find("div.analitics_result").html("");
			return  $(this).find('.errorResult').html("Оберiть Мегакульку!");
		}
		/*var addMB = $('select[name="megaball"]').val();
				if (addMB = ''){
					alert('Оберiть Мегакульку!');
				}*/
		var additionalData = '&mega_ball=' + $('select[name="megaball"]').val();
		/*if(additionalData==''){
				  alert('Оберiть Мегакульку!');
				}
				else{*/
		for (i = 0; i < selectedNumbers.length; i++) {
			additionalData += '&number[' + i + ']=' + selectedNumbers[i];
		}

		var that = this;

		/* var request = $(that).attr('action') + '?' + $(that).serialize() + additionalData; */
		/* window.open(request); */


		var request = $(that).serialize() + additionalData + "&ajax=1";
		jQuery.ajax({
		      url: "megalot.php",
			data: request,
			success: function(response) {
				$(that).find("div.analitics_result").html(response);
				$(that).find('.errorResult').html("");
				height_correction_1();
			},
		});

	});
	// function{ if (new_height>800) return '800'; else return new_height;}

	$("div#af_variant_2 form").submit(function(ev) {
		ev.preventDefault();
		if (selectedNumbers.length != 6) {
			var textError = '0 чисел';
			if (selectedNumbers.length == 1) {
				textError = selectedNumbers.length + ' число';
			} else if (selectedNumbers.length > 1 && selectedNumbers.length <= 4) {
				textError = selectedNumbers.length + ' числа';
			} else if (selectedNumbers.length > 4) {
				textError = selectedNumbers.length + ' чисел';
			}
			return $(this).find('.errorResult').html('Ви обрали ' + textError + ', а необхідно обрати 6 чисел!');
		}
		if (!$('select[name="megaball"]').val()) {
			return  $(this).find('.errorResult').html("Оберiть Мегакульку!");
		}

		var that = this;
		var additionalData = '&mega_ball=' + $('select[name="megaball"]').val();
		for (i = 0; i < selectedNumbers.length; i++) {
			additionalData += '&number[' + i + ']=' + selectedNumbers[i];
		}

		var request = $(that).serialize() + additionalData + "&ajax=1";
		jQuery.ajax({
		      url: "megalot.php",
			data: request,
			success: function(response) {
				$(that).find("div.analitics_result").html(response);
				$(that).find('.errorResult').html("");
				height_correction_2();
			},
		});
	});
</script>
</div>
        
     </div>
